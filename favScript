-------------------[DONT TOUCH]-----------------------------
local detect = game:GetService("Players"):FindFirstChild(Player)
local Char = detect.Character.UpperTorso.Position.X
local Char1 = detect.Character.UpperTorso.Position.Y
local Char2 = detect.Character.UpperTorso.Position.Z


Find = game.Players:FindFirstChild(Player)

if Find then
	print("Teleporting")
	game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1.2), {CFrame = CFrame.new(Char,Char1,Char2)}):Play()
end
